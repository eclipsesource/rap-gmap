<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="gmap"
         name="GMap"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.eclipsesource.widgets.gmaps.GMapDemo">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            applicationId="com.eclipsesource.widgets.gmaps.demo.gmap"
            id="com.eclipsesource.widgets.gmaps.demo.entrypoint"
            path="/gmaps">
      </entrypoint>
   </extension>

</plugin>
